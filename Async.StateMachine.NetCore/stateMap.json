{
  "initialState":"Ready",
  "stateDefinitions":[
    {
      "key":"Ready",
      "transitions":[{"trigger":"Processing","nextState":"Process"}]
    },
    {
      "key":"Process",
      "transitions":[
        {"trigger":"Success","nextState":"Result"},
        {"trigger":"Failure","nextState":"Error"}]},
    {
      "key":"Error",
      "transitions":[{"trigger":"Retry","nextState":"Process"}]},
    {
      "key":"Result",
      "transitions":[{"trigger":"Finish","nextState":"Ready"}]
    }]
}